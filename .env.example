# ===========================================
# Scraping Hub - Configuracao
# ===========================================

# Coresignal API
CORESIGNAL_API_KEY=your_coresignal_api_key_here
CORESIGNAL_BASE_URL=https://api.coresignal.com/cdapi/v1

# Proxycurl API
PROXYCURL_API_KEY=your_proxycurl_api_key_here
PROXYCURL_BASE_URL=https://nubela.co/proxycurl/api/v2

# Firecrawl API
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
FIRECRAWL_BASE_URL=https://api.firecrawl.dev

# Supabase
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_SERVICE_KEY=your_supabase_service_key
SUPABASE_ANON_KEY=your_supabase_anon_key

# Redis (opcional)
REDIS_URL=redis://localhost:6379/0

# Rate Limiting
CORESIGNAL_RATE_LIMIT=100
PROXYCURL_RATE_LIMIT=50
FIRECRAWL_RATE_LIMIT=200
RATE_LIMIT_PERIOD=60

# Cache
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Environment
ENVIRONMENT=development
